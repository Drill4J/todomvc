{
  "sources": {
    "pattern": [
      "js/**/*.ts"
    ],
    "ignore": [
      "**/js/libs/**",
      "**/js/interfaces/**"
    ]
  },
  "bundle": {
    "pattern": "js/**/*.js",
    "ignore": [
      "**/js/**/*.map",
      "**/js/libs/**",
      "**/js/interfaces/**"
    ]
  },
  "sourcemaps": {
    "pattern": "**/js/**/*.map",
    "ignore": [
      "**/js/libs/**",
      "**/js/interfaces/**"
    ]
  },
  "output": {
    "agentId": "todomvc-typescript-angular",
    "agentApiUrl": "http://jsagent:9404"
  }
}
